<div class="poll-question-container">
<h3 align=center aria-label="question" >Cast Your Vote</h3>
<div class="poll-question" data-poll_id="<%= @poll.id %>">
<h3 ><%= @poll.question %></h3>
<% @poll.question_options.each do |option| %>
<ul>
<li><a href="#" class="option" id="<%= option.id %>"><%= option.option %></a></li>
</ul>
<% end %>
</div>
</div>